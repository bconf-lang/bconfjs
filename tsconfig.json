{
	"compilerOptions": {
		"strict": true,
		"target": "esnext",
		"lib": ["ESNext", "DOM"],
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"resolveJsonModule": true,
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"noEmit": true
	},
	"include": ["src"]
}
